package com.iii.cb101.mt;

public class literal {

	public static void main(String[] args) {
		int x = 100;
		System.out.println(x);
		x = 0144;
		System.out.println(x);
		x = 0x64;
		System.out.println(x);
		double d = 3.14159;
		System.out.println(d);
		char c = 'A';
		System.out.println(c);
		c = '\u0042';
		System.out.println(c);
		c = '®a';
		System.out.println(c);
		//c = '®a®x';
		//System.out.println(c);//Invalid character constant
	}

}
